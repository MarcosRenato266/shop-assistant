    "merge-graphql-schemas": "^1.7.5",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "node": "^13.5.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.17.0",
    "puppeteer": "^2.0.0",
    "sequelize": "^5.21.3",
    "subscriptions-transport-ws": "^0.9.16",
    "uuid": "^3.3.3"
  }
