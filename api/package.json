{
  "name": "shop-assistant-api",
  "version": "0.0.1",
  "main": "./bin/index.js",
  "author": "Marcos Nunes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production node ./bin",
    "dev": "nodemon ./bin --exec babel-node",
    "lint": "eslint --ext .js src/",
    "test": "",
    "clean": "rm -rf dist && mkdir dist",
    "build": "yarn clean && babel src --out-dir dist --copy-files"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "setupFiles": [
      "./__tests__/testHelper.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "nodemon": "^2.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@types/node": "^13.1.2",
    "apollo-errors": "^1.9.0",
    "apollo-resolvers": "^1.4.1",
    "apollo-server-express": "^2.9.15",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "helmet": "^3.21.2",
